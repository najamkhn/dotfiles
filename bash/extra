# -----
# Some experimental aliases
# -----

alias memcached_flush='echo "flush_all" | nc localhost 11211'
alias whatbranch='git branch | grep "*"'
alias memc='/usr/local/bin/memcached -d -p 11211'
alias msqr='mysql.server stop;setmaxfiles;mysql.server start;'

